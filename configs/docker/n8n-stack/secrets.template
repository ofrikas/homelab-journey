# n8n Stack Environment Variables
# Copy this file to .env and replace the placeholder values with your own secure values
# NEVER commit the .env file to version control!

# PostgreSQL Configuration
POSTGRES_USER=n8n
POSTGRES_PASSWORD=REPLACE_ME_WITH_STRONG_PASSWORD
POSTGRES_DB=n8n

# n8n Security Keys
# Generate with: openssl rand -hex 32
N8N_ENCRYPTION_KEY=REPLACE_ME_WITH_RANDOM_HEX_STRING
N8N_USER_MANAGEMENT_JWT_SECRET=REPLACE_ME_WITH_RANDOM_HEX_STRING

# Timezone
TZ=Asia/Jerusalem
